#include "clock_manual.h"

int modifyStatus = MODIFY_SECOND;

void clock_manual_run() {
	switch(modifyStatus) {
		case MODIFY_SECOND:
			break;
		case MODIFY_MINUTE:
			break;
		case MODIFY_HOUR:
			break;
		case MODIFY_ARTICLE:
			break;
		case MODIFY_DAY:
			break;
		case MODIFY_MONTH:
			break;
		case MODIFY_YEAR:
			break;
		default:
			break;
	}
}
